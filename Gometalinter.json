{
    "Deadline": "5m",
    "DisableAll": true,
    "Enable": [
        "deadcode",
        "errcheck",
        "gas",
        "goconst",
        "gofmt",
        "golint",
        "ineffassign",
        "interfacer",
        "lll",
        "megacheck",
        "misspell",
        "nakedret",
        "safesql",
        "structcheck",
        "unconvert",
        "varcheck",
        "vet",
        "vetshadow"
    ],
    "Linters": {
        "vet": "go tool vet -printfuncs=Logf,Errorf,Infof,Infoerrf,Warningf :PATH:LINE:MESSAGE"
    },
    "LineLength": 120
}
