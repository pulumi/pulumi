{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "componentCallable.ts",
        "componentCustomRefInputOutput.ts",
        "componentCustomRefOutput.ts",
        "config/index.ts",
        "config/vars.ts",
        "custom.ts",
        "index.ts",
        "provider.ts",
        "utilities.ts"
    ]
}
