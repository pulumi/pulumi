{
  "$schema": "https://raw.githubusercontent.com/pulumi/pulumi/master/pkg/codegen/schema/pulumi.json",
  "name": "other",
  "version": "0.1.0",
  "resources": {
    "other:index:Thing": {
      "inputProperties": {
        "idea": {
          "type": "string"
        }
      }
    },
    "other:module:Object": {
      "inputProperties": {
        "answer": {
          "type": "number"
        }
      }
    }, 
    "pulumi:providers:other": {
      "inputProperties": {
        "name": {
          "type": "string"
        }
      }
    }
  },
  "language": {
    "nodejs": {
      "packageName": "@third-party/other"
    },
    "csharp": {
      "rootNamespace": "ThirdParty"
    },
    "python": {
      "packageName": "third_party"
    },
    "go": {
      "importBasePath": "git.example.org/thirdparty"
    }
  }
}
